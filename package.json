{
    "name": "n8n-nodes-folk",
    "version": "0.1.0",
    "description": "n8n community node for Folk CRM - manage people, companies, deals, notes, reminders, and more",
    "license": "MIT",
    "homepage": "https://folk.app",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "folk",
        "crm",
        "contacts",
        "deals"
    ],
    "author": {
        "name": "Romain Pattyn",
        "email": "romain.pattyn.ent@gmail.com"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/codika-io/n8n-nodes-folk.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "release": "n8n-node release",
        "prepublishOnly": "n8n-node prerelease"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "strict": true,
        "credentials": [
            "dist/credentials/FolkApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/FolkTrigger/FolkTrigger.node.js",
            "dist/nodes/Folk/Folk.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "*",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
